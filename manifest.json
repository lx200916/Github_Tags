{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0",
    "description": "Description of my extension",
    "permissions": ["storage", "scripting"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html"
    },
    "content_scripts": [
      {
        "matches": ["*://github.com/*"],
        "js": ["content.js"]
      }
    ],
    "background": {
      "service_worker": "background.js",
      "type": "module"

    },
    "options_ui": {
      "page": "options.html",
      "open_in_tab": true
    },"web_accessible_resources": [{
        "matches": ["*://github.com/*"],
        "resources": ["storage.js"]
      }]
    
  }
  